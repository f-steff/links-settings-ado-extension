{
    "manifestVersion": 1,
    "name": "Header Link Settings",
    "description": "Allows Project Collection Admins to set sitewide header links.",
    "categories": "Azure Boards",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        },
        {
            "id": "Microsoft.TeamFoundation.Server",
            "version": "[17.0,)"
        }
    ],
    "links": {},
    "icons": {
        "default": "../static/icon.png"
    },
    "content": {
        "license": {
            "path": "LICENSE"
        },
        "details": {
            "path": "README.md"
        }
    },
    "files": [
        {
            "path": "../static",
            "addressable": true
        },
        {
            "path": "../SETUP.md",
            "addressable": true
        },
        {
            "path": "../dist",
            "addressable": true
        }
    ],
    "scopes": [
        "vso.settings_write"
    ],
    "tags": [
        "Organization Settings",
        "Links",
        "Header",
        "Account",
        "Admin"
    ]
}
